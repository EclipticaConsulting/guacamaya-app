{
  "expo": {
    "name": "guacamaya-app",
    "slug": "guacamaya-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "guacamaya",
    "plugins": ["expo-router"],
    "splash": {
      "image": "./assets/guacamaya-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#171717"
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#171717"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ecliptica.guacamaya"
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ecliptica.guacamaya"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
